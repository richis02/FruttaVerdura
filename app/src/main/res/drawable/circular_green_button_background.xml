<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape =  "oval">
            <solid android:color="@color/green_text_icons" />
            <stroke
                android:dashGap="5dp"
                android:dashWidth="10dp"
                android:width="2.3dp"
                android:color="@color/light_yellow_green" />
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
        </shape>
    </item>
</layer-list>